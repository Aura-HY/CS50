#include <stdio.h>

int main()
{
    int hight;
    printf("hight:");
    scanf("%i",&hight);
    for(int i=0;i<hight;i++)
    {
        for(int j=hight-1;j>i;j--)
        {
             printf(" ");
        }
       
        for(int t=0;t<=i;t++)
        {
             printf("#");
        }
        printf("\n");
    }
}
